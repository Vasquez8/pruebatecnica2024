@model CapaEN.CategoriaEN

@{
    ViewData["Title"] = "Detalles de categoría";
}

<h1>Detalles de categoría</h1>

<div>
    <hr />
    <dl class="row">

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
